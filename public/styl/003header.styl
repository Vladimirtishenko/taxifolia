@import '001func.styl'

.taxifolia-inner-main-header
	width 100%
	background #fff

.taxifolia-inner-main-header
	width staticWidth
	margin 0 auto
	padding 20px 0
	display flex
	justify-content space-between
	align-items center
	
.taxifolia-item-menu
	display inline-block
	margin 0 13px
	&:first-child
		margin-left 30px

.taxifolia-item-menu a 
	color #000
	text-transform uppercase
	font-size 14px